{
  "name": "listnride-frontend-new",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start1": "ng serve --port 8080",
    "start1:https": "ng serve --port 8080 --ssl true --ssl-key /node_modules/browser-sync/certs/server.key --ssl-cert /node_modules/browser-sync/certs/server.crt",
    "config": "node set-env.js",
    "dev": "npm run config && ng serve",
    "start": "node server.js",
    "build": "npm run config && ng build",
    "test": "npm run config && ng test",
    "stylelint": "stylelint '**/*.scss'",
    "lint": "eslint --fix-dry-run 'src/**/*.ts'",
    "prettier": "prettier --check 'src/**/*.{ts,html}'",
    "prettier-fix": "prettier --write 'src/**/*.{ts,html}'",
    "lint-quiet": "eslint --fix-dry-run --quiet 'src/**/*.ts'",
    "lint-fix": "eslint --fix 'src/**/*.ts'",
    "e2e": "ng e2e",
    "heroku-postbuild": "npm run config && ng build --prod",
    "typecheck": "tsc --noEmit",
    "github-checks": "npm run prettier && npm run lint-quiet && npm run typecheck"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run github-checks"
    }
  },
